{
  "name": "@tyriar/splay-tree",
  "version": "2.0.4",
  "description": "An implementation of the splay tree data structure",
  "scripts": {
    "build": "tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "nyc -x '**/test/*' mocha lib/test/*.test.js",
    "watch": "tsc -w",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gwtw/ts-splay-tree.git"
  },
  "keywords": [
    "computer science",
    "data structure",
    "tree"
  ],
  "main": "lib/splayTree.js",
  "types": "typings/splay-tree.d.ts",
  "author": "Daniel Imms (http://www.growingwiththeweb.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gwtw/ts-splay-tree/issues"
  },
  "homepage": "https://github.com/gwtw/ts-splay-tree",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.3",
    "coveralls": "^3.0.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "tslint": "^5.9.1",
    "tslint-consistent-codestyle": "^1.13.0",
    "typescript": "3.6"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.js.map",
    "!lib/test",
    "src/**/*.ts",
    "!src/test",
    "typings/*.d.ts"
  ]
}
